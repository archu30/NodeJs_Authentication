
<link rel="stylesheet" href="/css/reset.css">
<div class="formContainer">

    <form class="form" action="/reset" method="post">
        <div class="img-div">
            <img class="reseting" src="https://img.icons8.com/?size=512&id=10480&format=png" alt="">


        </div>

        <h1 class="heading">Reset Your Password!</h1>
        <div><img src="" alt=""></div>
        <div class="inputField">
            <input class="inputFieldi" type="text" placeholder="Email" name="email" >
        </div>
        <div class="inputField">
            <input class="inputFieldi" type="password" name="old password" placeholder="Old Password">
        </div>
        <div class="inputField">
            <input class="inputFieldi" type="password" name="newpassword" placeholder="New Password">
        </div>

        <div class="btncontainer">
            <button class="btn" type="submit">Submit</button>

        </div>

    </form>
</div>
